  "next-unused": {
    "alias": {
      "@images/*": [
        "public/images/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@locale/*": [
        "src/locale/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
